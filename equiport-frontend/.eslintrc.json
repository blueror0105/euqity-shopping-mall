{
  "env": {
    "browser": true,
    "jest": true,
    "es2021": true
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": ["airbnb-typescript", "react-app", "react-app/jest", "eslint-config-next", "next/core-web-vitals", "next", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {}, //blank on purpose in order to remove configurations
      {
        "usePrettierrc": true,
        "withNodeModules": false
      }
    ]
  }
}